@model SimReg.Web.ViewModels.SIMREG_REQUESTEDTYPEViewModel
@using (Ajax.BeginForm("Edit", "RequestType", null,
        new AjaxOptions
        {
            UpdateTargetId = "updateTargetId",
            InsertionMode = InsertionMode.Replace,
            HttpMethod = "POST",
            OnSuccess = "EditRequestTypeSuccess",
        }, new { @id = "editRequestTypeForm" }))
{

    @Html.ValidationSummary(true)
    <div id="updateTargetId" class="error invisible">
    </div>
    @Html.HiddenFor(model => model.REQUESTEDTYPEID)
    <table width="100%">
        <tr>
            <td>@Html.LabelFor(model => model.TITLE)</td>
            <td>
                @Html.EditorFor(model => model.TITLE)
                @Html.ValidationMessageFor(model => model.TITLE)</td>
        </tr>
    </table>
}
